        <command>/macro book 5</command>
        <command delay="1000">/macro set 5</command>
        <command>/echo [Switching to RDM Macros]</command>
        <command>/bind F9 /tpmode</command>
        <command>/bind F10 /status</command>
        <command>/bind F12 /main</command>
        <command delay="4000">/lockstyle on</command>
    </init>
    <variables>
        <var name="tptype">Standard</var>
        <var name="weapon">0</var>
        <var name="main">Murgleis</var>
        <var name="sub">Genmei Shield</var>
    </variables>
    <inputcommands>
        <cmd input="/main">
            <if advanced="$weapon=2">
                <setvar name="main" value="Murgleis" />
                <setvar name="sub" value="Genmei Shield" />
                <setvar name="weapon" value="0" />
            </if>
            <elseif advanced="$weapon=1">
                <setvar name="main" value="Almace" />
                <setvar name="sub" value="Acclimator" />
                <setvar name="weapon" value="2" />
            </elseif>
            <else>
                <setvar name="main" value="Almace" />
                <setvar name="sub" value="Genmei Shield" />
                <setvar name="weapon" value="1" />
            </else>
            <doidlegear />
            <command>/echo [$main / $sub] [$tptype]</command>
        </cmd>
        <cmd input="/tpmode">
            <if advanced="$tptype=Defensive">
                <setvar name="tptype" value="Standard" />
            </if>
            <else>
                <setvar name="tptype" value="Defensive" />
            </else>
            <doidlegear />
            <command>/echo [$main / $sub] [$tptype]</command>
        </cmd>
        <cmd input="/status">
            <equip>
                <back>Agema Cape</back>
            </equip>
            <doidlegear />
            <command>/echo [$main / $sub] [$tptype]</command>
            <if buffactive="4|566">
                <command>/item "Panacea" me</command>
            </if>
            <elseif buffactive="9|15">
                <equip lock="true">
                    <ring2>Purity Ring</ring2>
                </equip>
                <command>/item "Hallowed Water" me</command>
            </elseif>
            <elseif buffactive="6">
                <command>/item "Echo Drops" me</command>
            </elseif>
            <elseif buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|174|175|404|557|558|559|560|561|562|563|564|567">
                <command>/item "Panacea" me</command>
            </elseif>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="engaged">
            <if p_joblevel="99">
                <equip set="$tptype" />
            </if>
            <else>
                <equip set="TP50" />
            </else>
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </idlegear>
    <preranged>
    </preranged>
    <midranged>
    </midranged>
    <premagic>
        <equip set="FastCast" />
    </premagic>
    <midmagic>
        <if ad_skill="elementalmagic">
            <if p_joblevel="99">
                <if advanced="(%ad_element=%e_dayelement)|(%ad_element=%e_weatherelement)">
                    <equip set="Nuke">
                        <waist augment="S7973728454464518565">Hachirin-no-Obi</waist>
                        <back>Twilight Cape</back>
                    </equip>
                </if>
                <else>
                    <equip set="Nuke" />
                </else>
            </if>
            <elseif p_joblevel=">70&<99">
                <if advanced="(%ad_element=%e_dayelement)|(%ad_element=%e_weatherelement)">
                    <equip set="Nuke71">
                        <waist augment="S7973728454464518565">Hachirin-no-Obi</waist>
                    </equip>
                </if>
                <else>
                    <equip set="Nuke71" />
                </else>
            </elseif>
            <elseif p_joblevel=">50&<71">
                <equip set="Nuke51" />
            </elseif>
            <else>
                <equip set="Nuke50" />
            </else>
        </if>
        <if ad_skill="enfeeblingmagic|bluemagic">
            <equip set="Enfeeble" />
        </if>
        <if ad_skill="darkmagic">
            <equip set="Nuke">
                <head>Appetence Crown</head>
            </equip>
        </if>
        <if ad_skill="enhancingmagic">
            <if ad_name="Refresh*">
                <equip set="Ref" />
            </if>
            <elseif ad_name="Phalanx*">
                <equip set="EnhPot" />
            </elseif>
            <elseif ad_name="en*">
            <!--
                Enspell Augment Bonus applies when the spell is cast, not when it procs on melee strike.
            -->
            <equip set="Enspell" />
            </elseif>
            <else>
                <equip set="Enh" />
            </else>                        
        </if>
        <if ad_name="Stoneskin|Metallic*|Diamondhide">
            <if buffactive="37">
                <command>/cancel 37</command>
            </if>
        </if>
        <if ad_name="Shock Spikes">
            <if buffactive="38">
                <command>/cancel 38</command>
            </if>
        </if>
    </midmagic>
    <jobability>
    </jobability>
    <weaponskill>
        <if ad_name="Uriel*|Seraph*|Shining*|Burning*|Red Lotus*|Sanguine*">
            <equip set="Nuke" />
        </if>
        <elseif ad_name="Requiescat">
            <equip set="WS">
                <ammo augment="S6176686717137314381">Minnow</ammo>
                <ear1>Telos Earring</ear1>
                <ear2 augment="S6176686717137314381">Mache Earring +1</ear2>
            </equip>
        </elseif>
        <else>
            <equip set="WS" />
        </else>
    </weaponskill>
</ashitacast>
