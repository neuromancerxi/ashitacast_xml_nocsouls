    <init>
        <if p_joblevel="99">
            <command>/macro book 9</command>
            <command delay="1000">/macro set 2</command>
            <command>/echo [Switching to BST Macros]</command>
            <command>/bind F9 /tpmode</command>
            <command>/bind F10 /status</command>
            <command>/bind F12 /main</command>
            <command delay="4000">/lockstyleset 27</command>
        </if>
        <else>
            <command>/ac load webjester_noob</command>
        </else>
    </init>
    <variables>
        <var name="weapon">0</var> <!-- Start in mode 0 (Farsha) -->
        <var name="tpmode">0</var> <!-- Start in Standard TP Mode -->
        <var name="tptype">Standard</var>
        <var name="main">Farsha</var> <!-- Default to Farsha before any mode switch -->
    </variables>
    <inputcommands>
        <cmd input="/main">
            <if advanced="$weapon=1">
                <setvar name="main" value="Farsha" />
                <setvar name="weapon" value="0" />
            </if>
            <else>
                <setvar name="main" value="Aymur" />
                <setvar name="weapon" value="1" />
            </else>
            <doidlegear />
            <command>/echo [$main] [$tptype]</command>
        </cmd>
        <cmd input="/tpmode">
            <if advanced="$tpmode=1">
                <setvar name="tpmode" value="0" />
                <setvar name="tptype" value="Standard" />
            </if>
            <else>
                <setvar name="tpmode" value="1" />
                <setvar name="tptype" value="Defensive" />
            </else>
            <doidlegear />
            <command>/echo [$main] [$tptype]</command>
        </cmd>
        <cmd input="/status">
            <command>/echo [$main] [$tptype]</command>
            <doidlegear />
            <if buffactive="4|566">
                <command>/item "Panacea" me</command>
            </if>
            <elseif buffactive="6">
                <command>/item "Echo Drops" me</command>
            </elseif>
            <elseif buffactive="9|15">
                <equip lock="true">
                    <ring2>Purity Ring</ring2>
                </equip>
                <command>/item "Hallowed Water" me</command>
            </elseif>
            <elseif buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|174|175|404|557|558|559|560|561|562|563|564|567">
                <command>/item "Panacea" me</command>
            </elseif>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="engaged">
            <if p_joblevel="99">
                <if advanced="$tpmode=1">
                    <equip set="DEFTP" />
                </if>
                <else>
                    <equip set="TP" />
                </else>
            </if>
            <else>
                <equip set="TP50" />
            </else>
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </idlegear>
    <preranged>
    </preranged>
    <midranged>
    </midranged>
    <premagic>
        <equip set="FastCast" />
    </premagic>
    <midmagic>
        <if ad_skill="bluemagic|enfeeblingmagic|elementalmagic|darkmagic">
            <equip set="MACC" />
        </if>
        <if ad_name="Stoneskin|Metallic*|Diamondhide">
            <if buffactive="37">
                <command>/cancel 37</command>
            </if>
        </if>
        <if ad_name="Shock Spikes">
            <if buffactive="38">
                <command>/cancel 38</command>
            </if>
        </if>
    </midmagic>
    <jobability>
    </jobability>
    <weaponskill>
        <if p_joblevel="99">
            <if ad_name="Primal Rend|Cloudsplitter">
                <equip set="MagWS" />
            </if>
            <else>
                <equip set="WS" />
            </else>
        </if>
        <else>
            <equip set="TP50" />
        </else>
    </weaponskill>
</ashitacast>
