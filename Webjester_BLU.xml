        <command>/macro book 14</command>
        <command delay="1000">/macro set 2</command>
        <command>/echo [Switching to BLU Macros]</command>
        <command>/bind F9 /tpmode</command>
        <command>/bind F10 /status</command>
        <command>/bind F12 /toggleweap</command>
        <command delay="4000">/lockstyleset 11</command>
    </init>
    <variables>
        <var name="Weapon">Acclimator</var>
        <var name="tptype">Standard</var>
    </variables>
    <inputcommands>
        <cmd input="/toggleweap">
            <if advanced="$Weapon=Acclimator">
                <setvar name="Weapon" value="Farm" />
            </if>
            <else>
                <setvar name="Weapon" value="Acclimator" />
            </else>
            <doidlegear />
            <command>/echo [$Weapon] [$tptype]</command>
        </cmd>
        <!--
        0 Standard TP
        1 Defensive TP
        -->
        <cmd input="/tpmode">
            <if advanced="$tptype=Defensive">
                <setvar name="tptype" value="Standard" />
            </if>
            <else>
                <setvar name="tptype" value="Defensive" />
            </else>
            <doidlegear />
            <command>/echo [$Weapon] [$tptype]</command>
        </cmd>
        <cmd input="/status">
            <equip>
                <back>Agema Cape</back>
            </equip>
            <doidlegear />
            <command>/echo [$Weapon] [$tptype]</command>
            <if buffactive="4|566">
                <command>/item "Panacea" me</command>
            </if>
            <elseif buffactive="9|15">
                <equip lock="true">
                    <ring2>Purity Ring</ring2>
                </equip>
                <command>/item "Hallowed Water" me</command>
            </elseif>
            <elseif buffactive="6">
                <command>/item "Echo Drops" me</command>
            </elseif>
            <elseif buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|174|175|404|557|558|559|560|561|562|563|564|567">
                <command>/item "Panacea" me</command>
            </elseif>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="engaged">
            <if p_joblevel="99">
                <equip set="$tptype" />
            </if>
            <else>
                <equip set="TP50" />
            </else>
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </idlegear>
    <preranged>
    </preranged>
    <midranged>
    </midranged>
    <premagic>
        <equip set="FastCast" />
    </premagic>
    <midmagic>
        <if ad_skill="bluemagic">
            <if ad_name="Uproot|Crashing Thunder|Thunderbolt|Tearing Gust|Droning Whirlwind|Cesspool|Polar Roar|Gates of Hades|Evryone. Grudge|Water Bomb|Charged Whisker|Magnetite Cloud">
                <equip set="MagWS" />
            </if>
            <else>
                <equip set="MACC">
                    <ring1 augment="S7973728454464518565">Stikini Ring +1</ring1>
                    <ring2 augment="S7973728454464518565">Stikini Ring +1</ring2>
                </equip>
            </else>
        </if>
        <if ad_skill="enfeeblingmagic|darkmagic">
            <equip set="MACC" />
        </if>
        <if ad_skill="elementalmagic">
            <equip set="MagWS" />
        </if>
        <if ad_name="en*">
            <!--
                Enspell Augment Bonus applies when the spell is cast, not when it procs on melee strike.
            -->
            <equip set="Enspell" />
        </if>
        <if ad_name="Stoneskin|Metallic*|Diamondhide">
            <if buffactive="37">
                <command>/cancel 37</command>
            </if>
        </if>
        <if ad_name="Shock Spikes">
            <if buffactive="38">
                <command>/cancel 38</command>
            </if>
        </if>
    </midmagic>
    <jobability>
    </jobability>
    <weaponskill>
        <if p_joblevel="99">
            <if ad_name="Uriel*|Seraph*|Shining*|Burning*|Red Lotus*|Sanguine*">
                <equip set="MagWS" />
            </if>
            <elseif ad_name="Chant*|Vorpal*">
                <equip set="WS">
                    <ear2>Telos Earring</ear2>
                </equip>
            </elseif>
            <else>
                <equip set="WS" />
            </else>
        </if>
        <else>
            <if ad_name="Uriel*|Seraph*|Shining*|Burning*|Red Lotus*|Sanguine*">
                <equip set="MagWS50" />
            </if>
            <else>
                <equip set="TP50" />
            </else>
        </else>
    </weaponskill>
</ashitacast>
